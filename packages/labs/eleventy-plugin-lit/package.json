{
  "name": "@lit-labs/eleventy-plugin-lit",
  "private": true,
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Eleventy plugin for rendering Lit components.",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/lit/lit.git",
    "directory": "packages/labs/eleventy-plugin-lit"
  },
  "author": "Google LLC",
  "homepage": "https://lit.dev/",
  "main": "index.js",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "default": "./index.js"
    }
  },
  "files": [
    "/index.{d.ts,d.ts.map,js,js.map}"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc --build && treemirror development . '**/*.d.ts{,.map}'",
    "build:ts:watch": "tsc --build --watch",
    "build:demo": "cd demo && eleventy",
    "build:demo:watch": "cd demo && eleventy --watch",
    "clean": "rimraf \"index.{js,map,d.ts}\" \"test/**/*.{js,map,d.ts}\" \"demo/_site\" \"demo/_js/*.bundle.js\" \"*.tsbuildinfo\"",
    "test": "uvu test \"_test.js$\" ",
    "serve:demo": "wds --watch --node-resolve --open demo/_site/test"
  },
  "dependencies": {
    "@lit-labs/ssr": "^2.0.1",
    "lit": "^2.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@web/dev-server": "^0.1.11",
    "@webcomponents/template-shadowroot": "^0.1.0",
    "internal-scripts": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "uvu": "^0.5.3"
  }
}
